#include <iostream>

void swapNumbers(int& a, int& b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int x = 10;
    int y = 5;

    std::cout << "Before swap: x = " << x << " y = " << y << std::endl;
    swapNumbers(x, y);
    std::cout << "After swap: x = " << x << " y = " << y << std::endl;

    return 0;
}
